{
    "include": ["vite.config.*", "src/types/*.d.ts", "src/**/*", "src/**/*.vue"],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
      "module": "es2022",
      "moduleResolution": "Node",
      "types": ["node"],
      "isolatedModules": false,
      "composite": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      }
    }
}